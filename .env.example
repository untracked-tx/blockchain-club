# .env.example

# Private key of the wallet that deploys/contracts/interacts
PRIVATE_KEY=your_private_key_here

# RPC URL for the Polygon Amoy testnet
AMOY_RPC_URL=https://rpc-amoy.polygon.technology

# Optional: Base URI for token metadata (can be IPFS or HTTPS)
BASE_URI=https://example.com/meta/

# Optional: Gnosis Safe address (if connected later)
TREASURY_ADDRESS=0xYourGnosisSafeAddress

# Optional: Snapshot space ID (for governance integration)
SNAPSHOT_SPACE=your-snapshot-space.eth
